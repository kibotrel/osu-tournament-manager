{
  "devDependencies": {
    "@commitlint/cli": "20.4.0",
    "@commitlint/config-conventional": "20.4.0",
    "@cspell/dict-fr-fr": "2.3.2",
    "@eslint/js": "9.39.2",
    "@packages/prettier-config": "workspace:*",
    "@types/node": "22.13.10",
    "@vitest/eslint-plugin": "1.6.6",
    "cspell": "9.6.2",
    "eslint": "9.39.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsdoc": "62.5.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-sonarjs": "3.0.6",
    "eslint-plugin-tsdoc": "0.5.0",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "17.2.0",
    "husky": "9.1.7",
    "nodemon": "3.1.11",
    "prettier": "3.8.1",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vitest": "4.0.18"
  },
  "name": "osu-tournament-manager",
  "scripts": {
    "format": "pnpm --filter=@packages/* format && pnpm --filter=./apps/* format",
    "lint": "pnpm --filter=@packages/* lint && pnpm --filter=./apps/* lint",
    "prepare": "pnpm husky && pnpm --filter=@packages/* build",
    "spellcheck": "cspell --config cspell.json --no-progress --no-summary --no-show-suggestions --no-must-find-files  --gitignore ./",
    "test": "vitest --silent"
  },
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
