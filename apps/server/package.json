{
  "dependencies": {
    "dotenv": "16.4.5",
    "drizzle-orm": "0.30.10",
    "express": "4.19.2",
    "pg": "8.11.5",
    "swagger-themes": "1.4.3",
    "swagger-ui-express": "5.0.0",
    "tsx": "4.9.3"
  },
  "devDependencies": {
    "@redocly/cli": "1.12.2",
    "@types/express": "4.17.21",
    "@types/pg": "8.11.6",
    "@types/swagger-ui-express": "4.1.6",
    "drizzle-kit": "0.21.0"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "name": "osu-tournament-manager/server",
  "private": true,
  "scripts": {
    "db:migrate": "drizzle-kit migrate",
    "db:migration:create": "NODE_OPTIONS='--import tsx' drizzle-kit generate:pg && node --loader ts-node/esm --no-warnings --env-file=.env ./src/scripts/renameMigrationFileScript.ts",
    "db:migration:drop": "drizzle-kit drop",
    "dev": "nodemon --config nodemon-server.json src/index.ts",
    "dev:specs": "nodemon --config nodemon-specs.json",
    "specs:bundle:internal": "redocly bundle internal@latest --output src/assets/internal.json",
    "specs:bundle:public": "redocly bundle public@latest --output src/assets/public.json"
  },
  "type": "module",
  "version": "0.0.0"
}
